    "postgh-pages": "GIT_DEPLOY_DIR=gh-pages GIT_DEPLOY_BRANCH=gh-pages node_modules/git-directory-deploy/deploy.sh -m 'Deploy gh-pages'; shx rm -rf gh-pages",
