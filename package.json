    "lint": "polylint -b node_modules/@browser_modules --no-recursion --input dom-deque.html",
    "docs": "npm run serve -- -o index.html",
    "demo": "npm run serve -- -o demo/index.html",
    "test": "npm run serve -- -o test/index.html"
